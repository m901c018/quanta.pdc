@model m_ExcelPartial

    <div class="table-responsive">
        <table id="grid" class="table table-hover display  nowrap table-striped" width="100%" cellspacing="0">
            <thead>
                <tr>
                    @foreach (var item in Model.StackupColumnList)
                    {
                        <th>@item.ColumnName</th>
                    }
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i <= Model.ExcelSheetDts[0].Rows.Count - 1; i++)
                {
                    <tr>
                        @for (int j = 0; j <= Model.StackupColumnList.Count - 1; j++)
                        {
                            <td>
                                <input type="text" value="@Model.ExcelSheetDts[0].Rows[i][j]" />
                            </td>

                            @*<td>
                                    <div class="switch d-inline m-r-10">
                                        <input type="checkbox" @(item.isSuperAdmin ? "checked" : "")>
                                        <label class="cr"></label>
                                    </div>
                                </td>*@

                        }
                    </tr>
                }
            </tbody>
        </table>
    </div>

<div class="input-append">
    <textarea rows="5" style="width:100%">
        @Model.Errmsg
    </textarea>
</div>